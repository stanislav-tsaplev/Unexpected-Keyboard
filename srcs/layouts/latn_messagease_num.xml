<?xml version="1.0" encoding="utf-8"?>
<!-- Clone of a numeric layout of MessagEase Keyboard -->
<keyboard name="MessagEase English" bottom_row="false" script="latin">
  <row height="0.8">
    <key key0="1" key3="$" key6="-" shift="2.6"/>
    <key key0="2" key1="`" key2="´" key3="/" key4="\" key5="+" key6="!" key7="^"/>
    <key key0="3" key4="€" key5="\?" key7="up" key8="="/>
    <key key0="ctrl" key7="config" width="1.15"/>
  </row>
  <row height="0.8">
    <key key0="4" key1="{" key2="%" key3="[" key4="_" key5="(" shift="2.6"/>
    <key key0="5"/>
    <key key0="6" key1="|" key2="}" key3="\@" key4="]" key6=")" key7="shift"/>
    <key key0="switch_numeric" key1="cut" key2="selectAll" key3="voice_typing" key5="switch_forward" key6="switch_backward" key7="copy" key8="paste" width="1.15"/>
  </row>
  <row height="0.8">
    <key key0="7" key1="~" key4="tab" key5="&lt;" key6="*" key7="¨" shift="2.6"/>
    <key key0="8" key1="&quot;" key2="'" key3="," key4=":" key8="."/>
    <key key0="9" key2="°" key3=";" key5="\#" key6="&gt;" key7="&amp;" key8="down"/>
    <key key0="backspace" key6="delete" width="1.15"/>
  </row>
  <row height="0.8">
    <key key0="0" width="2.0" shift="2.6"/>
    <key key0="space"/>
    <key key0="enter" key7="action" width="1.15"/>
  </row>
</keyboard>