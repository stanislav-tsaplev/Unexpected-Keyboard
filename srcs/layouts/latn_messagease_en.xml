<?xml version="1.0" encoding="utf-8"?>
<!-- Clone of a layout of MessagEase Keyboard for English language -->
<keyboard name="MessagEase English" bottom_row="false" script="latin">
  <row height="0.8">
    <key key0="a" key3="$" key4="v" key6="-" shift="2.6"/>
    <key key0="n" key1="`" key2="´" key3="/" key4="\" key5="+" key6="!" key7="^" key8="l"/>
    <key key0="i" key3="x" key4="€" key5="\?" key7="up" key8="="/>
    <key key0="ctrl" key7="config" width="1.15"/>
  </row>
  <row height="0.8">
    <key key0="h" key1="{" key2="%" key3="[" key4="_" key5="(" key6="k" shift="2.6"/>
    <key key0="o" key1="q" key2="p" key3="g" key4="j" key5="c" key6="b" key7="u" key8="d"/>
    <key key0="r" key1="|" key2="}" key3="\@" key4="]" key5="m" key6=")" key7="shift"/>
    <key key0="switch_numeric" key1="cut" key2="selectAll" key3="voice_typing" key5="switch_forward" key6="switch_backward" key7="copy" key8="paste" width="1.15"/>
  </row>
  <row height="0.8">
    <key key0="t" key1="~" key2="y" key4="tab" key5="&lt;" key6="*" key7="¨" shift="2.6"/>
    <key key0="e" key1="&quot;" key2="'" key3="," key4=":" key6="z" key7="w" key8="."/>
    <key key0="s" key1="f" key2="°" key3=";" key5="\#" key6="&gt;" key7="&amp;" key8="down"/>
    <key key0="backspace" key6="delete" width="1.15"/>
  </row>
  <row height="0.8">
    <key key0="space" key5="cursor_left" key6="cursor_right" slider="true" width="3.0" shift="2.6"/>
    <key key0="enter" key7="action" width="1.15"/>
  </row>
</keyboard>